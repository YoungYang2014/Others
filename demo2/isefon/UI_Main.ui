/* === QPushButton === */
QPushButton {
    padding: 4px;
    min-width: 65px;
    min-height: 12px;
}

QPushButton:hover {
}

QPushButton:pressed {
    color: yellow;
}

QPushButton:disabled {
    color: #333333;
}

/* === Checkable items === */
QCheckBox::indicator, QRadioButton::indicator, QTreeView::indicator {
    width: 16px;
    height: 16px;
